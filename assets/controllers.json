{
    "controllers": {
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        },
        "app": {
            "command_palette": {
                "enabled": true,
                "fetch": "lazy"
            },
            "infinite_scroll": {
                "enabled": true,
                "fetch": "lazy"
            },
            "theme": {
                "enabled": true,
                "fetch": "lazy"
            },
            "pwa": {
                "enabled": true,
                "fetch": "lazy"
            }
        }
    },
    "entrypoints": []
}
